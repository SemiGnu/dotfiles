# ls
alias ll='ls -alFh'
alias la='ls -A'
alias l='ls -CF'

# kubectl
alias kns=k8s_set_namespace
alias k=kubectl
alias kcuc='kubectl config use-context'
alias kccc='kubectl config current-context'
alias kcgc='kubectl config get-contexts'
alias kaf='kubectl apply -f'

# git
alias gcp='git commit -p'
alias gpl='git pull'
alias gff='git pull --ff-onll'
alias gps='git push'

# dotnet ef
alias def='dotnet ef'
alias defma='dotnet ef migrations add'
alias defmr='dotnet ef migrations remove'
alias defdu='dotnet ef database update'

# python
alias py='python3'

